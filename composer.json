{
    "name": "veerajyadav/cbsp-plugin",
    "description": "A WordPress plugin for choosing and displaying WooCommerce best-selling products.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "version": "1.0.0",
    "require": {
      "php": ">=7.2"
    },
    "homepage": "https://github.com/veerajxcode/chooseyourbestsellingproducts",
    "authors": [
        {
            "name": "veerajxcode",
            "email": "veeraj11yadav@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "wp-coding-standards/wpcs": "^3.1",
        "phpcompatibility/phpcompatibility-wp": "^2.1"
    },
    "config": {
      "allow-plugins": {
        "dealerdirect/phpcodesniffer-composer-installer": true
      }
    },
    "autoload": {
      "psr-4": {
        "CBSP\\": "includes/"
      }
    }
  }
